<Window x:Class="orderMatchingHome.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:orderMatchingHome"
        mc:Ignorable="d"
        Title="MainWindow" Height="Auto" Width="Auto">
    <StackPanel>
        <DockPanel Height="30" VerticalAlignment="Top" >
            
            <Label DockPanel.Dock="Left" x:Name="labelCiti" Content="Cti"  Width="Auto" FontSize="16"  />
            <Label DockPanel.Dock="Right" x:Name="labelOMS"  Content="Order Matching System" HorizontalContentAlignment="Right" FontSize="16"/>
        </DockPanel>
        <WrapPanel Height="472">
            <Menu Height="466" MinWidth="100" MaxWidth="200">
                <StackPanel>
                    <MenuItem Header="DASHBOARD" Click="MenuItem_Click_4" />
                    <MenuItem Header="TRADE" Click="MenuItem_Click"/>
                    <MenuItem Header="EXECUTED" Click="MenuItem_Click_1"/>
                    <MenuItem Header="TRANSACTION LOG" Click="MenuItem_Click_2"/>
                    <MenuItem Header="PENDING" Click="MenuItem_Click_3"/>
                </StackPanel>
            </Menu>
            <Frame Name="frameHome" Width="700"></Frame>

        </WrapPanel>


    </StackPanel>
</Window>
